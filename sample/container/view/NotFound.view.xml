<mvc:View 
    controllerName="sample.container.view.NotFound"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc" >
    <Page 
        id="notFoundPage" 
        title="{i18n>notFoundTitle}" 
        enableScrolling="false">
        <headerContent>
            <!--<ndis:HelpProvider />-->
        </headerContent>
        <content>
            <Text 
                id="notFoundText" 
                class="spacerTop" 
                text="Not Found VCRContainer" 
                textAlign="Center" 
                width="100%" 
                maxLines="0">
            </Text>
        </content>
        <footer
            id="notFoundFooter">
            <Toolbar 
                id="notFoundToolbar">
            </Toolbar>
        </footer>
    </Page>
</mvc:View>